<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!--Psychopathic-->
<GeneDef>
    <defName>Psychopathic</defName>
	<label>psychopathic</label>
	<description>Carriers of this gene are naturally numb to violence and human suffering.</description>
	<iconPath>Genes/CYB_Psychopathic</iconPath>
    <displayCategory>CYB_Social</displayCategory>
    <displayOrderInCategory>41</displayOrderInCategory>
    <forcedTraits>
      <li>
        <def>Psychopath</def>
      </li>
    </forcedTraits>
	<descriptionHyperlinks>
		<TraitDef>Psychopath</TraitDef>
	</descriptionHyperlinks>
    <biostatCpx>1</biostatCpx>
    <symbolPack>
      <prefixSymbols>
        <li><symbol>psycho</symbol></li>
        <li><symbol>cold</symbol></li>
        <li><symbol>numb</symbol></li>
      </prefixSymbols>
      <suffixSymbols>
        <li><symbol>killer</symbol></li>
      </suffixSymbols>
    </symbolPack>
</GeneDef>

<!--Delightful-->
<GeneDef>
	<defName>CYB_Delightful</defName>
	<label>delightful</label>
    <displayCategory>CYB_Social</displayCategory>
    <displayOrderInCategory>45</displayOrderInCategory> 
	<description>Carriers of this gene possess a natural charm and charisma and tend to be liked by everyone they meet. The right thing to say comes easily to them, and they will often lift the spirits of those around them with a quip or compliment.</description>
	<iconPath>Genes/CYB_Delightful</iconPath>
    <forcedTraits>
      <li>
        <def>CYB_Delightful</def>
      </li>
    </forcedTraits>
	<descriptionHyperlinks>
		<TraitDef>CYB_Delightful</TraitDef>
	</descriptionHyperlinks>
    <biostatCpx>2</biostatCpx>
	<biostatMet>-1</biostatMet>
    <symbolPack>
      <prefixSymbols>
        <li><symbol>bright</symbol></li>
        <li><symbol>glee</symbol></li>
        <li><symbol>joy</symbol></li>
        <li><symbol>love</symbol></li>
      </prefixSymbols>
      <suffixSymbols>
        <li><symbol>light</symbol></li>
        <li><symbol>love</symbol></li>
        <li><symbol>shine</symbol></li>
      </suffixSymbols>
    </symbolPack>
</GeneDef>


