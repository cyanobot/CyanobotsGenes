<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <GeneDef ParentName="GeneSkinColorOverride" Name="CYB_GeneSkinColorOverride" Abstract="True">
	<randomBrightnessFactor>0</randomBrightnessFactor>
    <displayOrderInCategory>300</displayOrderInCategory>
    <modExtensions>
		<li Class="VanillaGenesExpanded.GeneExtension">
			<hideGene>false</hideGene>
		</li>
	</modExtensions>
  </GeneDef>
  
  <GeneDef ParentName="CYB_GeneSkinColorOverride">
	<defName>CYB_SkinBundle_BlueBlack</defName>
	<label>blue-black tones</label>
	<description>Carriers of this gene may have skin of a number of blue-black shades.</description>
	<geneClass>CyanobotsGenes.Gene_Bundle</geneClass>
	<iconPath>Genes/SkinBundles/SkinBundle_BlueBlack</iconPath>
	<modExtensions>
		<li Class="CyanobotsGenes.GeneExtension_Bundle">
			<genes>
				<li>CYB_Skin_AbyssBlue</li>
				<li>CYB_Skin_MidnightBlue</li>
				<li>CYB_Skin_SlateBlue</li>
			</genes>
		</li>
	</modExtensions>
  </GeneDef>

  <GeneDef ParentName="CYB_GeneSkinColorOverride">
    <defName>CYB_Skin_AbyssBlue</defName>
    <label>abyss blue skin</label>
    <labelShortAdj>abyss</labelShortAdj>
    <description>Carriers of this gene produce a pigment that turns their skin a dark blue reminiscent of the oceanic abyss.</description>
    <skinColorOverride>(46, 57, 66)</skinColorOverride>
    <symbolPack>
      <prefixSymbols>
        <li><symbol>blue</symbol></li>
        <li><symbol>dark</symbol></li>
        <li><symbol>abyss</symbol></li>
      </prefixSymbols>
    </symbolPack>
  </GeneDef>

