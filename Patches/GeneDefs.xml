<?xml version="1.0" encoding="utf-8" ?>
<Patch>
  <Operation Class="CyanobotsGenes.PatchOperationBySetting">
	<setting>moveVanillaGenes</setting>
	<on Class="PatchOperationReplace">
		<xpath>Defs/GeneDef[defName="RobustDigestion"]/displayCategory</xpath>
		<value>
			<displayCategory>Diet</displayCategory>
		</value>
	</on>
	<off Class="PatchOperationReplace">
		<xpath>Defs/GeneDef[defName="RobustDigestion"]/displayCategory</xpath>
		<value>
			<displayCategory>Miscellaneous</displayCategory>
		</value>
	</off>
  </Operation>
  
  <Operation Class="CyanobotsGenes.PatchOperationBySetting">
	<setting>moveVanillaGenes</setting>
	<on Class="PatchOperationReplace">
		<xpath>Defs/GeneDef[defName="StrongStomach"]/displayCategory</xpath>
		<value>
			<displayCategory>Diet</displayCategory>
		</value>
	</on>
	<off Class="PatchOperationReplace">
		<xpath>Defs/GeneDef[defName="RobustDigestion"]/displayCategory</xpath>
		<value>
			<displayCategory>Miscellaneous</displayCategory>
		</value>
	</off>
  </Operation>
  
  <Operation Class="CyanobotsGenes.PatchOperationBySetting">
	<setting>moveVanillaGenes</setting>
	<on Class="PatchOperationReplace">
		<xpath>Defs/GeneDef[defName="KindInstinct"]/displayCategory</xpath>
		<value>
			<displayCategory>Social</displayCategory>
		</value>
	</on>
	<off Class="PatchOperationReplace">
		<xpath>Defs/GeneDef[defName="KindInstinct"]/displayCategory</xpath>
		<value>
			<displayCategory>Violence</displayCategory>
		</value>
	</off>
  </Operation>
  
  
  <Operation Class="CyanobotsGenes.PatchOperationBySetting">
	<setting>moveVanillaGenes</setting>
	<on Class="PatchOperationReplace">
		<xpath>Defs/GeneDef[defName="Furskin"]/displayCategory</xpath>
		<value>
			<displayCategory>Fur</displayCategory>
		</value>
	</on>
	<off Class="PatchOperationReplace">
		<xpath>Defs/GeneDef[defName="Furskin"]/displayCategory</xpath>
		<value>
			<displayCategory>Miscellaneous</displayCategory>
		</value>
	</off>
  </Operation>
  
</Patch>